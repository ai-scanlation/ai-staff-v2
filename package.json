{
    "name": "ai.staff",
    "version": "2.1.0",
    "license": "MIT",
    "scripts": {
        "dev": "electron-webpack dev",
        "build": "electron-webpack && electron-builder",
        "build:dir": "yarn build --dir -c.compression=store -c.mac.identity=null",
        "build:dir:run": "yarn build:dir && yarn open",
        "open": "dist/win-unpacked/ai.staff.exe"
    },
    "electronWebpack": {
        "main": {
            "sourceDirectory": "src/main"
        },
        "renderer": {
            "webpackConfig": "src/renderer/webpack.config.js",
            "sourceDirectory": "src/renderer"
        }
    },
    "build": {
        "appId": "net.ai.staff",
        "win": {
            "icon": "icon.ico"
        }
    },
    "dependencies": {
        "animejs": "^2.2.0",
        "facebook-chat-api": "^1.4.0",
        "google-auth-library": "^1.3.1",
        "googleapis": "^27.0.0",
        "lokijs": "^1.5.3",
        "moment": "^2.21.0",
        "mousetrap": "^1.6.1",
        "queue": "^4.4.2",
        "shelljs": "^0.8.1",
        "source-map-support": "^0.5.3",
        "vue": "^2.5.14",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1",
        "vuex-persistedstate": "^2.4.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-root-import": "^5.1.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "css-loader": "^0.28.10",
        "devtron": "^1.4.0",
        "electron": "1.8.2",
        "electron-builder": "^20.4.1",
        "electron-devtools-installer": "^2.2.3",
        "electron-webpack": "^1.13.0",
        "eslint": "^4.18.2",
        "eslint-config-standard": "^11.0.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "eslint-plugin-vue": "^4.3.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.0.6",
        "json-loader": "^0.5.7",
        "node-sass": "^4.7.2",
        "npm-run-all": "^4.1.2",
        "raw-loader": "^0.5.1",
        "sass-loader": "^6.0.7",
        "url-loader": "^1.0.1",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^14.2.1",
        "vue-template-compiler": "^2.5.14",
        "webpack": "^3.11.0",
        "webpack-sources": "^1.1.0"
    },
    "resolutions": {
        "webpack-sources": "1.0.1"
    }
}
